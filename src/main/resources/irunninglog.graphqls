schema {
    query: Query
}

type Query {
    athlete: Athlete!
}

type Athlete {
    id: String!
    firstName: String
    lastName: String
    city: String
    state: String
    country: String
    sex: Sex
    profileSmall: String
    profileLarge: String
    shoes: [ShoeSummary]
}

enum Sex {
    Male,
    Female
}

type ShoeSummary {
    id: ID!
    name: String!
    primary: Boolean!
}